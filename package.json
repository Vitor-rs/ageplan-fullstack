{
  "name": "ageplan-monorepo",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento para freelancers e criadores de conte√∫do",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/backend-api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/backend-api/test/jest-e2e.json",
    "dev:backend": "nest start backend-api --watch",
    "dev:frontend": "cd apps/frontend-client && npm run start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:backend": "nest build backend-api",
    "build:frontend": "cd apps/frontend-client && npm run build",
    "build:all": "npm run build:backend && npm run build:frontend"
  },
  "keywords": [],
  "author": "Vitor Santos",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/swagger": "^11.0.7",
    "@wppconnect-team/wppconnect": "^1.36.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.13.11",
    "@types/passport-jwt": "^4.0.1",
    "concurrently": "^9.1.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
